{
	"name": "poschuler.com",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"cf-typegen": "wrangler types",
		"deploy": "npm run build && wrangler deploy",
		"dev": "react-router dev",
		"postinstall": "npm run cf-typegen",
		"preview": "npm run build && vite preview",
		"typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
		"kv:generate": "node ./seed/kv/generate-kv-json.ts",
		"kv:upload:local": "node ./seed/kv/kv-bulk-upload.ts local",
		"kv:upload:remote": "node ./seed/kv/kv-bulk-upload.ts remote",
		"kv:seed:local": "npm run kv:generate && npm run kv:upload:local",
		"kv:seed:remote": "npm run kv:generate && npm run kv:upload:remote",
		"d1:generate": "node ./seed/d1/generate-seed-sql.ts",
		"d1:upsert:local": "npx wrangler d1 execute poschuler --file ./seed/d1/seed.sql --local",
		"d1:upsert:remote": "npx wrangler d1 execute poschuler --file ./seed/d1/seed.sql --remote",
		"d1:seed:local": "npm run d1:generate && npm run d1:upsert:local",
		"d1:seed:remote": "npm run d1:generate && npm run d1:upsert:remote"
	},
	"dependencies": {
		"@forge42/seo-tools": "^1.4.5",
		"@radix-ui/colors": "^3.0.0",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-toast": "^1.2.15",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"front-matter": "^4.0.2",
		"isbot": "^5.1.31",
		"lucide-react": "^0.556.0",
		"marked": "^17.0.1",
		"react": "^19.1.2",
		"react-aria-components": "^1.13.0",
		"react-dom": "^19.1.2",
		"react-router": "^7.10.0",
		"remix-themes": "^2.0.4",
		"remix-toast": "^3.4.0",
		"remix-utils": "^9.0.0",
		"tailwind-merge": "^3.4.0",
		"tailwindcss-react-aria-components": "^2.0.1",
		"tw-animate-css": "^1.4.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.13.5",
		"@cloudflare/workers-types": "^4.20251209.0",
		"@react-router/cloudflare": "^7.10.1",
		"@react-router/dev": "^7.10.0",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^22",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"tailwindcss": "^4.1.13",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.53.0"
	}
}
